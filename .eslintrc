{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "indent": [
      "error",
      2
    ],
    "semi": [
      "error"
    ],
    "no-console": [
      "off"
    ],
    "no-underscore-dangle": [
      0
    ]
  },
  "globals": {}
}